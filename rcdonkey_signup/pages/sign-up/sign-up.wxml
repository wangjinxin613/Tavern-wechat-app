<view class="page sign-up div">
  <form bindsubmit="onFormSubmit" bindreset="onFormReset">
    <view class="avatar">
      <image class="img-avatar" src="data:image/png;base64,"></image>
    </view>
    <view class="edit">
      编辑昵称，让我知道你是谁
      <image class="img-edit" src="data:image/png;base64,"></image>
    </view>
    <view class="modal modal-conform">
      <view class="modal-dialog">
        <view class="modal-body">
            <view class="title">
                编辑昵称
            </view>
            <input class="weui-input" type="text" placeholder="请输入文本"/>
        </view>
        <view class="modal-footer">
            <navigator url="" class="btn btn-success">确定</navigator>
        </view>
      </view>
      <view class="mask"></view>
    </view>
    <textarea class="weui-textarea textarea" name="remark" placeholder="请写下您的建设性建议，提交给主办方，如果没有，直接提交按钮，即可报名成功" rows="10" />
    <view class="weui-cells weui-cells_form">
      <view class="weui-cell">
        <view class="weui-cell__hd"><label class="weui-label">真实姓名</label></view>
        <view class="weui-cell__bd">
          <input class="weui-input" type="text" placeholder="请输入姓名"/>
        </view>
      </view>
      <view class="weui-cell">
        <view class="weui-cell__hd"><label class="weui-label">手机号</label></view>
        <view class="weui-cell__bd">
          <input class="weui-input" type="number" placeholder="请输入手机号"/>
        </view>
      </view>
      <view class="weui-cell">
        <radio-group class="radio-group" bindchange="radioChange">
          <label class="radio" wx:for="{{items}}">
            <radio value="{{item.name}}" checked="{{item.checked}}"/>{{item.value}}
          </label>
        </radio-group>
      </view>
    </view>
    <view class="weui-btn-area div">
      <button  formType="submit" style="color:#ffffff" class="weui-btn weui-btn_primary button">
        报名
      </button>
    </view>
  </form>
</view>